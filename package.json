{
  "name": "pushpulljs",
  "version": "1.0.3",
  "description": "Convenience tool to make your files content flexible",
  "bin": {
    "pushpull": "./bin/cli.js"
  },
  "main": "./src/index.js",
  "files": [
    "bin",
    "src"
  ],
  "scripts": {
    "lint": "eslint . --fix",
    "test": "jest",
    "prepublishOnly": "npm run lint && npm run test",
    "update-package-lock": "node bin/cli.js .npmrc --switch '#UPDATE' && npm i && npm ddp && node bin/cli.js .npmrc --switch '#UPDATE'"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=6.0.0"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.19.1"
  },
  "keywords": [
    "cli",
    "tool",
    "flexible",
    "config"
  ],
  "author": "Richard Lindner <rlindner81@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/rlindner81/pushpull.git"
  },
  "bugs": {
    "url": "https://github.com/rlindner81/pushpull/issues"
  }
}
